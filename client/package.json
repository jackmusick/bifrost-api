{
    "name": "client",
    "version": "0.0.1",
    "type": "module",
    "description": "Open-source automation engine for MSPs.",
    "author": "Jack Musick <jack@gocovi.com>",
    "private": true,
    "scripts": {
        "dev": "node watch-config.js & swa start http://localhost:5173 --run \"vite\" --api-devserver-url http://localhost:7071",
        "dev:docker": "node watch-config.js & swa start http://localhost:5173 --run \"vite\" --api-devserver-url http://functions",
        "build": "tsc -b && vite build",
        "build:prod": "cp staticwebapp.config.prod.json staticwebapp.config.json && npm run build",
        "tsc": "tsc -b",
        "preview": "swa start dist",
        "lint": "eslint .",
        "deploy": "npm run build:prod && swa deploy",
        "generate:types": "npx openapi-typescript http://localhost:7071/api/openapi.json -o src/lib/v1.d.ts"
    },
