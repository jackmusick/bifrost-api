{
    "version": "2.0",
    "functionTimeout": "02:00:00",
    "logging": {
        "logLevel": {
            "default": "Warning",
            "Worker.LanguageWorkerChannel.LanguageWorkerChannel": "Error",
            "Worker": "Error",
            "Function": "Information",
            "Host": "Warning",
            "Azure": "Error",
            "azure.core.pipeline.policies.http_logging_policy": "WARNING"
        },
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            }
        }
    },
    "extensionBundle": {
        "id": "Microsoft.Azure.Functions.ExtensionBundle",
        "version": "[4.*, 5.0.0)"
    },
    "extensions": {
        "queues": {
            "maxDequeueCount": 3,
            "visibilityTimeout": "00:05:00"
        }
    }
}
