{
    "version": "2.0",
    "functionTimeout": "00:10:00",
    "logging": {
        "logLevel": {
            "default": "Information",
            "Host": "Warning",
            "Function": "Information",
            "Host.Aggregator": "Warning",
            "Azure": "Error",
            "Microsoft.Azure.SignalR": "Information"
        },
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            }
        }
    },
    "extensionBundle": {
        "id": "Microsoft.Azure.Functions.ExtensionBundle",
        "version": "[4.*, 5.0.0)"
    },
    "extensions": {
        "queues": {
            "maxDequeueCount": 3,
            "visibilityTimeout": "00:05:00"
        }
    }
}
