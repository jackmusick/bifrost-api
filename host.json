{
    "version": "2.0",
    "functionTimeout": "02:00:00",
    "logging": {
        "logLevel": {
            "default": "Information",
            "Function": "Information",
            "Azure": "Error"
        },
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            }
        }
    },
    "extensionBundle": {
        "id": "Microsoft.Azure.Functions.ExtensionBundle",
        "version": "[4.*, 5.0.0)"
    },
    "extensions": {
        "queues": {
            "maxDequeueCount": 3,
            "visibilityTimeout": "00:05:00"
        }
    }
}
